{
    "notifications_start":{
        "type":"single",
        "process":{
            "notif_request_2":{
                "notif_request_2":{
                    "module":"notification"
                }
            }
        }
    },
    "generate_mp4":{
        "type":"parallel",
        "process":{
            "br1":{
                "encode_mp4":{
                    "module":"encode",
                    "profile":"mp4_hq_1200k",
                    "input_file":"%INPUT_FILE%",
                    "output_file":"%TMP_FILE%_BR1"
                },
                "package_mp4":{
                    "module":"package",
                    "tool":"mp4box",
                    "input_file":"%TMP_FILE%_BR1",
                    "output_file":"%OUTPUT_FILE%",
                    "file_settings":{
                        "suffix":"_1200k",
                        "extension":"mp4"
                    }
                }
            },
            "br2":{
                "encode_mp4":{
                    "module":"encode",
                    "profile":"mp4_sd_400k",
                    "input_file":"%INPUT_FILE%",
                    "output_file":"%TMP_FILE%_BR2"
                },
                "package_mp4":{
                    "module":"package",
                    "tool":"mp4box",
                    "input_file":"%TMP_FILE%_BR2",
                    "output_file":"%OUTPUT_FILE%",
                    "file_settings":{
                        "suffix":"_400k",
                        "extension":"mp4"
                    }
                }
            }
        }
    },
    "notifications":{
        "type":"single",
        "process":{
            "notif_request":{
                "notif_request":{
                    "module":"notification"
                }
            }
        }
    }
}